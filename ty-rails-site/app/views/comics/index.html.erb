<div class="header-wrapper">
  <div>
    <%= link_to image_tag("ty-home-image.jpg", :alt => ""), "/" %>
  </div>
</div>
<section>
  <h1 class="page-header">COMICS</h1>
  <% @comics.each do |series| %>
    <div class="comics-cube-wrapper">
      <div class="cube-top-left"></div>
      <div class="cube-bottom-right"></div>
      <div class="content-cube">
        <h1 class="index-link"><%= link_to "#{series.upcase_name}", "comics/#{series.id}" %></h1>
      </div>
    </div>
  <% end %>
</section>